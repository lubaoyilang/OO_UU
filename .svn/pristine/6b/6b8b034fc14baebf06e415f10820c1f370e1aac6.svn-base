#import <UIKit/UIKit.h>
#import "UIImageView+WebCache.h"

@interface UIImageView (ImageSize)

/**
 *  自定义图片大小的加载
 *
 *  @param urlString  图片网址
 *  @param sizeString 图片大小格式  //640x320
 *  @param options    SDWebImageOptions
 *  @param imageName  placeHolderImageName 默认图片名称
 */
- (void)sz_setImageWithUrlString:(NSString *)urlString imageSize:(NSString *)sizeString options:(SDWebImageOptions)options placeholderImageName:(NSString *)imageName;

@end
