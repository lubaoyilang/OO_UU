#import <Foundation/Foundation.h>

@interface NSTimer (Addition)

//暂停 定时器
- (void)pauseTimer;

//重新启动 定时器
- (void)resumeTimer;

//在interval秒后 重新启动 定时器
- (void)resumeTimerAfterTimeInterval:(NSTimeInterval)interval;

@end
